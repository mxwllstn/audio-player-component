{
  "name": "@mxwllstn/audio-player-vue",
  "type": "module",
  "version": "0.0.78",
  "description": "Vue component for embedding audio files and streams",
  "author": "Max Stein <maxwell.stein@gmail.com> (https://maxstein.net",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mxwllstn/audio-player-vue"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/audio-player.mjs"
      },
      "require": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/audio-player.cjs"
      }
    }
  },
  "main": "./dist/audio-player.cjs",
  "module": "./dist/audio-player.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublish": "yarn lint && yarn build"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.1",
    "@babel/types": "^7.24.0",
    "@types/node": "^20.11.24",
    "@types/svgo": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.57.0",
    "postcss-html": "^1.6.0",
    "ress": "^5.0.2",
    "sass": "^1.72.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-css-injected-by-js": "^3.4.0",
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.4.21",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.6"
  }
}
